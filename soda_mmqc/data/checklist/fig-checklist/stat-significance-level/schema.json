{
    "format": {
        "type": "json_schema",
        "name": "stat-significance-level",
        "schema": {
            "type": "object",
            "properties": {
                "outputs": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "panel_label": {
                                "type": "string",
                                "description": "Label of the panel (e.g., A, B, C)"
                            },
                            "is_a_plot": {
                                "type": "string",
                                "enum": ["yes", "no"],
                                "description": "Whether the figure is a plot"
                            },
                            "significance_level_symbols_on_image": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                },
                                "description": "Significance level symbols on the image"
                            },
                            "significance_level_symbols_defined_in_caption": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                },
                                "description": "Significance level symbols defined in the caption"
                            },
                            "from_the_caption": {
                                "type": "string",
                                "description": "Extracted text explaining the significance level symbols"
                            }
                        },
                        "required": [
                            "panel_label",
                            "is_a_plot",
                            "significance_level_symbols_on_image",
                            "significance_level_symbols_defined_in_caption",
                            "from_the_caption"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "required": ["outputs"],
            "additionalProperties": false
        },
        "strict": true
    }
}