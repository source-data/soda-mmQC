{
    "format": {
        "type": "json_schema",
        "name": "micrograph-symbols-defined",
        "schema": {
            "type": "object",
            "properties": {
                "outputs": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "panel_label": {
                                "type": "string",
                                "description": "Label of the panel (e.g., A, B, C)"
                            },
                            "micrograph": {
                                "type": "string",
                                "enum": ["yes", "no"],
                                "description": "Whether the panel is a micrograph, a picture of a microscopic sample, microscopy images"
                            },
                            "symbols": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                },
                                "description": "Symbols present on the image"
                            },
                            "symbols_defined_in_caption": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": ["yes", "no"]
                                },
                                "description": "Whether symbols are defined in caption"
                            },
                            "from_the_caption": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                },
                                "description": "Text from caption describing symbols, when symbols are present"
                            }
                        },
                        "required": [
                            "panel_label",
                            "micrograph",
                            "symbols",
                            "symbols_defined_in_caption",
                            "from_the_caption"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "required": ["outputs"],
            "additionalProperties": false
        },
        "strict": true
    }
}