{
    "format": {
        "type": "json_schema",
        "name": "individual-data-points",
        "schema": {
            "type": "object",
            "properties": {
                "outputs": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "panel_label": {
                                "type": "string",
                                "description": "Label of the panel (e.g., A, B, C)"
                            },
                            "plot": {
                                "type": "string",
                                "enum": ["yes", "no"],
                                "description": "Whether the panel is a plot"
                            },
                            "average_values": {
                                "type": "string",
                                "enum": ["yes", "no", "not needed"],
                                "description": "Whether the panel displays average values"
                            },
                            "individual_values": {
                                "type": "string",
                                "enum": ["yes", "no", "not needed"],
                                "description": "Whether the panel displays individual data points"
                            }
                        },
                        "required": [
                            "panel_label",
                            "plot",
                            "average_values",
                            "individual_values"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "required": ["outputs"],
            "additionalProperties": false
        },
        "strict": true
    }
}